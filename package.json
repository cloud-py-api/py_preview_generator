{
	"name": "py_preview_generator",
	"description": "Preview generator (using Python) for Nextcloud",
	"version": "0.1.0",
	"keywords": [
		"python",
		"nextcloud",
		"preview",
		"generator"
	],
	"homepage": "https://github.com/cloud_py_api/py_preview_generator",
	"bugs": {
		"url": "https://github.com/cloud_py_api/py_preview_generator/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cloud-py-api/py_preview_generator"
	},
	"license": "AGPL-3.0",
	"private": true,
	"author": "Alexander Piskun <bigcat88@icloud.com>",
	"contributors": [
		"Andrey Borysenko <andrey18106x@gmail.com>",
		"Alexander Piskun <bigcat88@icloud.com>"
	],
	"scripts": {
		"build": "NODE_ENV=production webpack --progress --config webpack.js",
		"dev": "NODE_ENV=development webpack --progress --config webpack.js",
		"watch": "NODE_ENV=development webpack --progress --watch --config webpack.js",
		"lint": "eslint --ext .js,.vue src",
		"lint:fix": "eslint --ext .js,.vue src --fix",
		"stylelint": "stylelint src/**/*.vue src/**/*.scss src/**/*.css",
		"stylelint:fix": "stylelint src/**/*.vue src/**/*.scss src/**/*.css --fix"
	},
	"browserslist": [
		"extends @nextcloud/browserslist-config"
	],
	"dependencies": {
		"@nextcloud/auth": "^2.0.0",
		"@nextcloud/axios": "^2.0.0",
		"@nextcloud/dialogs": "^3.1.2",
		"@nextcloud/initial-state": "^2.0.0",
		"@nextcloud/l10n": "^2.0.1",
		"@nextcloud/moment": "^1.1.1",
		"@nextcloud/router": "^2.0.0",
		"@nextcloud/vue": "^7.0.0",
		"@nextcloud/vue-dashboard": "^2.0.1",
		"@nextcloud/vue-richtext": "^2.1.0-beta.6",
		"vue": "^2.6.14",
		"vue-click-outside": "^1.1.0",
		"vue-clipboard2": "^0.3.3",
		"vue-material-design-icons": "^5.1.2"
	},
	"devDependencies": {
		"@nextcloud/babel-config": "^1.0.0",
		"@nextcloud/browserslist-config": "^2.2.0",
		"@nextcloud/eslint-config": "^8.0.0",
		"@nextcloud/stylelint-config": "^2.1.2",
		"@nextcloud/webpack-vue-config": "^5.1.0",
		"eslint-webpack-plugin": "^3.1.1",
		"stylelint-webpack-plugin": "^3.3.0"
	},
	"engines": {
		"node": "^16.0.0",
		"npm": "^7.0.0 || ^8.0.0"
	}
}
